{
  "name": "iterable-range",
  "version": "3.2.0",
  "description": "Sequential positive range iterator with map, filter, and take helper functions",
  "main": "lib",
  "scripts": {
    "test": "nyc npm run run-tests",
    "run-tests": "babel-tape-runner **/*.test.js | faucet",
    "build": "rm -rf ./lib && babel src --out-dir lib --ignore test.js",
    "prepare": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hillc5/range-iterator.git"
  },
  "keywords": [
    "range",
    "iterable",
    "generator",
    "lazy",
    "filter",
    "map"
  ],
  "author": "Charlie Hill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hillc5/range-iterator/issues"
  },
  "homepage": "https://github.com/hillc5/range-iterator#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-tape-runner": "^2.0.1",
    "faucet": "0.0.1",
    "nyc": "^12.0.2",
    "regenerator-runtime": "^0.12.0",
    "sinon": "^6.1.3",
    "tape": "^4.9.1"
  }
}
